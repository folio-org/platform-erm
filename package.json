{
  "name": "@folio/platform-erm",
  "version": "2.1.2",
  "license": "Apache-2.0",
  "repository": "folio-org/platform-erm",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "scripts": {
    "build": "export NODE_OPTIONS=\"--max-old-space-size=2048 $NODE_OPTIONS\"; stripes build stripes.config.js",
    "stripes": "stripes",
    "start": "stripes serve stripes.config.js",
    "build-module-descriptors": "stripes mod descriptor stripes.config.js --output ./ModuleDescriptors",
    "local": "f=stripes.config.js; test -f $f.local && f=$f.local; echo Using config $f; stripes serve $f",
    "test": "echo 'No unit tests implemented'",
    "test-int": "stripes test nightmare stripes.config.js",
    "lint": "eslint test/ui-testing"
  },
  "dependencies": {
    "@folio/agreements": "~2.1.0",
    "@folio/erm-usage": "~2.0.0",
    "@folio/licenses": "~2.3.0",
    "@folio/orders": "~1.3.1",
    "@folio/plugin-find-agreement": "~2.1.0",
    "@folio/plugin-find-license": "~3.1.0",
    "@folio/plugin-find-user": "~1.6.0",
    "@folio/plugin-find-vendor": "~1.3.2",
    "@folio/stripes": "~2.1.0",
    "@folio/stripes-erm-components": "~1.2.1",
    "@folio/tags": "~1.3.0",
    "@folio/users": "~2.21.0",
    "@folio/vendors": "~1.5.0",
    "react": "~16.6.3",
    "react-dom": "~16.6.3",
    "react-redux": "~5.1.1",
    "redux": "~3.7.2"
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": "~3.2.0",
    "@folio/stripes-cli": "^1.10.0",
    "eslint": "^5.12.0",
    "moment": "^2.22.2"
  }
}
